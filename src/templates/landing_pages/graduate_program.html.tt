<footer class="center-text primary-bg-color content-inverse-color">
    <p>[% l("Browse all career opportunities at <a class='link' target='_blank' href='[_1]'>[_2]</a>", request.url_for('careers.html'), website_name) %]</p>
</footer>

<script src="[% request.url_for('js/landing_pages/common.js') _ '?' _ static_hash %]"></script>
<script src="[% request.url_for('js/landing_pages/graduate_program.js') _ '?' _ static_hash %]"></script>
</body>
