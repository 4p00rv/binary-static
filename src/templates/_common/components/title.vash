@{
    view.title = () => {
        const browser_title = model.title ? `${html.L(model.title)} | ` : '';
        const is_ja = model.language.toLowerCase() === 'ja';
        const title = `${browser_title}${is_ja ? html.L('{JAPAN ONLY}Binary.com page title') : model.website_name}`;
        <title>@title</title>
    }
}